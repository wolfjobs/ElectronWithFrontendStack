{
  "name": "electronwithfrontendstack",
  "private": true,
  "version": "0.0.0",
  "main": "src/main/main.js",
  "type": "module",
  "scripts": {
    "build": "tsc -b && vite build",
    "dev": "vite",
    "electron": "cross-env NODE_ENV=development electron .",
    "lint": "eslint .",
    "preview": "vite preview",
    "start": "concurrently -k 'npm run dev' 'npm run electron'"
  },
  "dependencies": {
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "@vitejs/plugin-vue": "^5.2.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "svelte": "^5.19.0",
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "electron": "^34.0.0",
    "eslint": "^9.17.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "globals": "^15.14.0",
    "postcss": "^8.5.1",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.18.2",
    "vite": "^6.0.5",
    "vite-plugin-html": "^3.2.2"
  },
  "build": {
    "appId": "com.example.app",
    "productName": "ElectronWithFrontendStack",
    "directories": {
      "output": "dist"
    },
    "files": [
      "dist/**/*",
      "main.js",
      "preload.js"
    ],
    "win": {
      "target": "nsis"
    },
    "mac": {
      "target": "dmg"
    },
    "linux": {
      "target": "deb"
    }
  }
}
